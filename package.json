{
  "name": "clinic-template",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:fast": "vite build --config vite.config.fast.cjs",
    "preview": "vite preview",
    "clean": "rm -rf node_modules dist .vite package-lock.json",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "build:pwa": "vite build && npm run generate-sw",
    "generate-sw": "vite build && npx workbox generateSW workbox-config.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@uiw/react-signature": "^1.3.2",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-react": "^4.6.0",
    "compressorjs": "^1.2.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.11",
    "idb-keyval": "^6.2.2",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-html5-camera-photo": "^1.5.11",
    "react-icons": "^5.5.0",
    "react-input-mask": "^2.0.4",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.22.3",
    "resend": "^4.7.0",
    "uuid": "^11.1.0",
    "vite": "^5.4.19",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
